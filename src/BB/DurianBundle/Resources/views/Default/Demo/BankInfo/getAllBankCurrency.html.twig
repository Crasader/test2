{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get All BankCurrency</h1>
    <code>GET /api/bank_info/currency</code>
</div>

<p class="lead">回傳全部的銀行幣別資訊</p>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-3">enable</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="enable" type="text" class="form-control" value="1" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">0:回傳停用銀行，1:回傳啟用銀行，不傳則全部回傳。</span>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button data-request-routing="api_all_bank_info_currency" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
