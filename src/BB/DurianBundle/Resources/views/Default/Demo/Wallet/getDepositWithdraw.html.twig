{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get Deposit Withdraw</h1>
    <code>GET /api/wallet/deposit_withdraw</code>
</div>

<p class="lead">取得使用者存提款紀錄</p>

<form class="form-horizontal">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-3">user_id</label>
            <div class="col-md-5">
                <input name="user_id" type="text" class="form-control">
            </div>
            <span class="help-block col-md-4">使用者編號(必填)</span>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-6">
                <button data-request-routing="api_get_deposit_withdraw" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
