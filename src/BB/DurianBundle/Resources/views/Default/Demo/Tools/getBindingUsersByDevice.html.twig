{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get Binding Users By Device</h1>
    <code>GET /api/tools/device/users</code>
</div>

<div class="row">
    <div class="col-md-7">
        <p class="lead">列出裝置所有綁定使用者</p>
        <p></p>
    </div>
</div>

<div class="alert alert-info">
    此API僅供RD5檢查使用
</div>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-3">app_id</label>
            <div class="col-md-5">
                <input name="app_id" type="text" class="form-control">
            </div>
            <span class="help-block col-md-4">裝置識別ID</span>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button data-request-routing="api_tools_get_binding_users_by_device" data-request-type="GET" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
