{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Set Payment Withdraw Fee</h1>
    <code>PUT /api/payment_charge/{id}/withdraw_fee</code>
</div>

<p class="lead">修改線上支付設定的出款手續費</p>

<form class="form-horizontal">
