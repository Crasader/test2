        <div class="form-group">
            <label class="control-label col-md-2"></label>
            <div class="col-md-7">
                <div class="input-group">
                    <span class="input-group-addon">/api/payment_charge/</span>
                    <input data-request-param="id" type="text" class="form-control" placeholder="{id}">
                    <span class="input-group-addon">/withdraw_fee</span>
                </div>
            </div>
            <p class="help-block col-md-offset-2 col-md-10">輸入網址變數{id}</p>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">free_period</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="free_period" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">重覆出款時數</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">free_count</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
