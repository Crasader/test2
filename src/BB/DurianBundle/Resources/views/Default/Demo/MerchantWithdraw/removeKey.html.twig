{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Remove Merchant Withdraw Key</h1>
    <code>DELETE /api/merchant/withdraw/key/{keyId}</code>
</div>

<p class="lead">移除出款商家金鑰檔案</p>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-2"></label>
            <div class="col-md-6">
                <div class="input-group">
                    <span class="input-group-addon">/api/merchant/withdraw/key/</span>
                    <input data-request-param="keyId" type="text" class="form-control" placeholder="{keyId}">
                </div>
            </div>
            <p class="help-block col-md-offset-2 col-md-10">輸入網址變數{keyId}</p>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button data-request-routing="api_remove_merchant_withdraw_key" data-request-type="DELETE" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
