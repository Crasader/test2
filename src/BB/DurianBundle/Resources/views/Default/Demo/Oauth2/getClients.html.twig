{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get All Oauth2 Client</h1>
    <code>GET /api/oauth2/client</code>
</div>

<p class="lead">回傳 Oauth2 client 一覽</p>

<form class="form-horizontal">
    <fieldset>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button data-request-routing="api_oauth2_get_clients" data-request-type="GET" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
