{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get Merchant Withdraw Level List</h1>
    <code>GET /api/merchant/withdraw/{merchantWithdrawId}/level/list</code>
</div>

<p class="lead">回傳出款商家層級列表</p>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-2"></label>
            <div>
                <div class="col-md-7">
                    <div class="input-group">
                        <span class="input-group-addon">/api/merchant/withdraw/</span>
                        <input data-request-param="merchantWithdrawId" type="text" class="form-control" placeholder="{merchantWithdrawId}">
                        <span class="input-group-addon">/level/list</span>
                    </div>
                </div>
                <p class="help-block col-md-offset-2 col-md-10">輸入網址變數{merchantWithdrawId}</p>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button data-request-routing="api_get_merchant_withdraw_level_list" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
