{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get MerchantWithdraw Level BankInfo</h1>
    <code>GET /api/merchant/withdraw/level/bank_info</code>
</div>

<p class="lead">回傳出款商家層級出款銀行</p>
<div class="alert alert-info">
    參數<code>domain</code>, <code>merchant_withdraw_id</code>需擇一帶入
</div>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-3">domain</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="domain" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">廳</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">merchant_withdraw_id</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="merchant_withdraw_id" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">出款商家ID</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">level_id</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="level_id" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">層級ID</span>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button data-request-routing="api_get_merchant_withdraw_level_bank_info" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
