{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get Ip Blacklist By Id</h1>
    <code>GET /api/domain/ip_blacklist/{ipBlacklistId}</code>
</div>
<div class="row">
    <div class="col-md-7">
        <p class="lead">回傳單一IP封鎖列表</p>
    </div>
</div>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-2"></label>
            <div class="col-md-6">
                <div class="input-group">
                    <span class="input-group-addon">/api/domain/ip_blacklist/</span>
                    <input data-request-param="ipBlacklistId" type="text" class="form-control" placeholder="{ipBlacklistId}">
                </div>
            </div>
            <p class="help-block col-md-offset-2 col-md-10">輸入網址變數{ipBlacklistId}</p>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button data-request-routing="api_get_ip_blacklist_by_id" data-request-type="GET" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
