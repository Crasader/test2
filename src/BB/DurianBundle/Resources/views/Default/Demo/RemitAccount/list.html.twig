{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get Remit Account List</h1>
    <code>GET /api/remit_account/list</code>
</div>

<p class="lead">回傳出入款列表</p>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-3">domain</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="domain" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">廳</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">bank_info_id</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="bank_info_id" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">銀行ID</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">account</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="account" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">帳號</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">account_type</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="account_type" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">帳號類別</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">auto_remit_id</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="auto_remit_id" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">自動認款平台ID</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">auto_confirm</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="auto_confirm" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">自動確認</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">crawler_on</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="crawler_on" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">爬蟲停啟用。0:停用，1:啟用</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">crawler_run</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="crawler_run" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">爬蟲執行狀態。0:非執行中，1:執行中</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">crawler_update_start</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="crawler_update_start" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">搜尋爬蟲最後執行時間起始時間，請使用ISO-8601格式輸入</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">crawler_update_end</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="crawler_update_end" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">搜尋爬蟲最後執行時間結束時間，請使用ISO-8601格式輸入</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">currency</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="currency" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">幣別</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">enable</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="enable" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">啟停用</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">suspend</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="suspend" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">是否暫停</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">deleted</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="deleted" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">已刪除</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">level_id</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="level_id" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">層級ID</span>
        </div>
        <hr>
        <div class="form-group">
            <label class="control-label col-md-3">first_result</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="first_result" type="text" class="form-control" value="0" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">分頁從第幾筆開始</span>
            <label class="control-label col-md-3">max_results</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="max_results" type="text" class="form-control" value="20" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">分頁顯示筆數</span>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-6">
                <button data-request-routing="api_remit_account_list" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
