{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get Bitcoin Deposit Entries List</h1>
    <code>GET /api/bitcoin_deposit/entry/list</code>
</div>

<p class="lead">入款記錄列表</p>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-3">currency</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="currency" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">幣別</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">bitcoin_wallet_id</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="bitcoin_wallet_id" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">比特幣錢包id</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">bitcoin_address_id</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="bitcoin_address_id" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">比特幣位址id</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">bitcoin_address</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="bitcoin_address" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">比特幣入款位址</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">user_id</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="user_id" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">會員id</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">domain</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="domain" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">廳</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">level_id</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="level_id" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">層級</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">process</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="process" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">處理中</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">confirm</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="confirm" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">已確認</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">cancel</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="cancel" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">已取消</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">locked</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="locked" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">已鎖定</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">amount_entry_id</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="amount_entry_id" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">現金明細id</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">control</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="control" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">控端操作，預設為0</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">operator</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="operator" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">操作者</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">amount_min</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="amount_min" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">入款金額最小值</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">amount_max</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="amount_max" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">入款金額最大值</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">bitcoin_amount_min</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="bitcoin_amount_min" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">比特幣入款金額最小值</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">bitcoin_amount_max</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                        <input name="bitcoin_amount_max" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">比特幣入款金額最大值</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">at_start</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="at_start" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">搜尋開始時間。ISO-8601格式</span>
            <label class="control-label col-md-3">at_end</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="at_end" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">搜尋結束時間。ISO-8601格式</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">confirm_at_start</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="confirm_at_start" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">確認入款搜尋開始時間。ISO-8601格式</span>
            <label class="control-label col-md-3">confirm_at_end</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="confirm_at_end" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">確認入款搜尋結束時間。ISO-8601格式</span>
        </div>
        <hr>
        <div class="form-group">
            <button class="btn btn-default action-more" type="button">複製參數</button>
            <div class="template">
                <label class="control-label col-md-3">sort[]</label>
                <div class="col-md-5">
                    <div class="input-group">
                        <span class="input-group-addon">
                            <input class="action-enable" type="checkbox">
                        </span>
                        <input name="sort[]" type="text" class="form-control" value="id" disabled>
                    </div>
                </div>
                <span class="help-block col-md-4">排序欄位</span>
                <label class="control-label col-md-3">order[]</label>
                <div class="col-md-5">
                    <div class="input-group">
                        <span class="input-group-addon">
                            <input class="action-enable" type="checkbox">
                        </span>
                        <input name="order[]" type="text" class="form-control" value="asc" disabled>
                    </div>
                </div>
                <span class="help-block col-md-4">升冪或降冪。可設定asc或desc</span>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">first_result</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="first_result" type="text" class="form-control" value="0" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">分頁從第幾筆開始</span>
            <label class="control-label col-md-3">max_results</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="max_results" type="text" class="form-control" value="20" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">分頁顯示筆數</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">sub_total</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="sub_total" type="text" class="form-control" value="0" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">是否回傳小計。預設0</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">total</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="total" type="text" class="form-control" value="0" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">是否回傳總計。預設0</span>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button data-request-routing="api_bitcoin_deposit_entry_list" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
