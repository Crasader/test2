{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Enable</h1>
    <code>PUT /api/cash_fake/{cashFakeId}/enable</code>
</div>

<p class="lead">啟用快開額度</p>
<div class="alert alert-info">回傳balance已扣除pre_sub預扣，另顯示pre_sub預扣 pre_add預存</div>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-2"></label>
            <div class="col-md-6">
                <div class="input-group">
                    <span class="input-group-addon">/api/cash_fake/</span>
                    <input data-request-param="cashFakeId" type="text" class="form-control" placeholder="{cashFakeId}">
                    <span class="input-group-addon">/enable</span>
                </div>
            </div>
            <p class="help-block col-md-offset-2 col-md-10">輸入網址變數{cashFakeId}</p>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button data-request-routing="api_cash_fake_enable" data-request-type="PUT" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
