{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get Level</h1>
    <code>GET /api/level/{levelId}</code>
</div>

<div class="row">
    <div class="col-md-7">
        <p class="lead">回傳單筆層級資料</p>
    </div>
    {% include 'BBDurianBundle:Default/Doc:merchant_order_strategy.html.twig' %}
</div>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-2"></label>
            <div class="col-md-6">
                <div class="input-group">
                    <span class="input-group-addon">/api/level/</span>
                    <input data-request-param="levelId" type="text" class="form-control" placeholder="{levelId}">
                </div>
            </div>
            <p class="help-block col-md-offset-2 col-md-10">輸入網址變數{levelId}</p>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-6">
                <button data-request-routing="api_get_level" data-request-type="GET" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
