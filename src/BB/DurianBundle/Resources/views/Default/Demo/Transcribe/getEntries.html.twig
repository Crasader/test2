{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get Transcribe Entries</h1>
    <code>GET /api/transcribe/entries</code>
</div>

<p class="lead">取得人工抄錄明細</p>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-3">account_id</label>
            <div class="col-md-5">
                <input name="account_id" type="text" class="form-control">
            </div>
            <span class="help-block col-md-4">出入款帳號id</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">booked_at_start</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="booked_at_start" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">搜尋帳目時間,起。ISO8601格式</span>
            <label class="control-label col-md-3">booked_at_end</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="booked_at_end" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">搜尋帳目時間,迄。ISO8601格式</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">first_transcribe_at_start</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="first_transcribe_at_start" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">搜尋首次抄錄時間,起。ISO8601格式</span>
            <label class="control-label col-md-3">first_transcribe_at_end</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="first_transcribe_at_end" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">搜尋首次抄錄時間,迄。ISO8601格式</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">confirm_at_start</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="confirm_at_start" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">搜尋認領時間,起。ISO8601格式</span>
            <label class="control-label col-md-3">confirm_at_end</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="confirm_at_end" type="text" class="form-control" value="{{ "now"|date("Y-m-d\\TH:i:sO") }}" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">搜尋認領時間,迄。ISO8601格式</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">blank</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="blank" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">空資料</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">confirm</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="confirm" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">已確認</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">withdraw</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="withdraw" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">出款</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">deleted</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="deleted" type="text" class="form-control" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">已刪除</span>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">first_result</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="first_result" type="text" class="form-control" value="0" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">分頁從第幾筆開始</span>
            <label class="control-label col-md-3">max_results</label>
            <div class="col-md-5">
                <div class="input-group">
                    <span class="input-group-addon">
                        <input class="action-enable" type="checkbox">
                    </span>
                    <input name="max_results" type="text" class="form-control" value="20" disabled>
                </div>
            </div>
            <span class="help-block col-md-4">分頁顯示筆數</span>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-6">
                <button data-request-routing="api_get_transcribe_entries" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
