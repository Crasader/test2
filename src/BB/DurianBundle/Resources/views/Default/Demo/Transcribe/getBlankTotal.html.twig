{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get Transcribe Blank Total</h1>
    <code>GET /api/transcribe/account/{accountId}/blank_total</code>
</div>

<p class="lead">依入款帳號取得人工抄錄明細空資料的總數</p>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-2"></label>
            <div class="input-group col-md-7">
                <span class="input-group-addon">/api/transcribe/account/</span>
                <input data-request-param="accountId" type="text" class="form-control" placeholder="{accountId}">
                <span class="input-group-addon">/blank_total</span>
            </div>
            <p class="help-block col-md-offset-2 col-md-10">輸入網址變數{accountId}</p>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-6">
                <button data-request-routing="api_get_transcribe_blank_total" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
