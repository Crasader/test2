{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Get Transcribe Max Rank</h1>
    <code>GET /api/transcribe/max_rank</code>
</div>

<p class="lead">依入款帳號取得人工抄錄明細中的最大排序</p>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-3">account_id</label>
            <div class="col-md-5">
                <input name="account_id" type="text" class="form-control">
            </div>
            <span class="help-block col-md-4">出入款帳號id</span>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-6">
                <button data-request-routing="api_get_transcribe_max_rank" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
