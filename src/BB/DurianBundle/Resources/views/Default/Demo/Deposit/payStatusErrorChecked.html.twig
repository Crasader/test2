{% extends 'BBDurianBundle:Default/Demo:index.html.twig' %}

{% block content %}
<div class="page-header">
    <h1>Pay Status Error Checked</h1>
    <code>PUT /api/deposit/pay_status_error_checked</code>
</div>

<p class="lead">修改異常確認入款明細執行狀態</p>

<form class="form-horizontal" role="form">
    <fieldset>
        <div class="form-group">
            <label class="control-label col-md-3">operator</label>
            <div class="col-md-5">
                <input name="operator" type="text" class="form-control">
            </div>
            <span class="help-block col-md-4">操作者</span>
        </div>
        <div class="form-group">
            <button class="btn btn-default action-more" type="button">複製參數</button>
            <div class="template">
                <label class="control-label col-md-3">entry_id[]</label>
                <div class="col-md-5">
                    <input name="entry_id[]" type="text" class="form-control">
                </div>
                <span class="help-block col-md-4">入款明細ID</span>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button data-request-routing="api_deposit_pay_status_error_checked" data-request-type="PUT" class="btn btn-primary" type="button" data-loading-text="loading...">Request</button>
            </div>
        </div>
    </fieldset>
</form>
{% endblock %}
