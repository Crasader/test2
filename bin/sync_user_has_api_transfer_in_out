#!/bin/sh

MAX=$1
BIN=$(cd $(dirname "$0"); pwd)

# 最大同時跑的數量預設為20個
if [ -z "$MAX" ]
then
    MAX=20
fi

if [ $MAX -gt "`ps aux | grep 'sync-cash-fake --api-transfer-in-out' | grep -v grep | wc -l`" ]
then
    $BIN/../app/console durian:sync-cash-fake --api-transfer-in-out --env=prod >> $BIN/../app/logs/sync_user_has_api_transfer_in_out.log 2>&1 &
fi

